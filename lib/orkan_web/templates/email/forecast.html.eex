<html>
  <head>
  </head>
  <body>
    <h1>Orkan wind alert</h1>
    <%= for forecast <- @forecasts do %>
      <h2><%= forecast.place %>
        <%= for place_forecasts <- forecast.forecasts do %>
          <div>
            <%= place_forecasts.datetime %>
            <%= place_forecasts.wind_direction %>
            <%= place_forecasts.wind_speed %>
          </div>
        <% end %>
      <table>
      </table>
    <% end %>
  </body>
</html>
